/* pages/map/map.wxss */

.biggerMap {
  position: fixed;
  margin: 0 0;
  z-index: 11;
  top: 0; 
  width: 100%; 
  height: 100%; 
  left: 0;
}

.fix_map_return {
  position: fixed;
  width: 80rpx;
  height: 80rpx;
  margin: 4% 5%;
  top: 0;
  left: 0;
}

.fix_map_local {
  position: fixed;
  width: 80rpx;
  height: 80rpx;
  margin: 4% 5%;
  bottom: 0;
  right: 0;
}